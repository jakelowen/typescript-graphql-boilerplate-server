    "typeorm": "0.2.7",
    "uuid": "^3.2.1",
    "yup": "^0.25.1"
  },
  "scripts": {
    "start": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "build": "rimraf ./dist && tsc && copyfiles -u 1 src/**/*.graphql dist",
    "test-server": "NODE_ENV=test ts-node src/index.ts",
    "gen-schema-types": "ts-node src/scripts/createTypes.ts",
    "psql": "psql -U postgres graphql-ts-server-boilerplate",
    "test": "NODE_ENV=test jest --forceExit",
    "gen-db-schema-types": "schemats generate -c postgres://postgres@localhost/graphql-ts-server-boilerplate -o src/types/dbschema.ts",
    "postinstall": "yarn build"
  },
  "jest": {
    "globalSetup": "./src/testUtils/callSetup.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  }
}
